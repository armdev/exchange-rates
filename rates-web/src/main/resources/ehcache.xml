<ehcache>
    
    <defaultCache
        maxElementsInMemory="500"
        eternal="false"
        overflowToDisk="false"
        overflowToOffHeap="false"
        memoryStoreEvictionPolicy="LRU" >
        <persistence strategy="none" />
    </defaultCache>

    <cache name="live.cache" 
           maxElementsInMemory="500" 
           eternal="false"
           overflowToDisk="false"
           overflowToOffHeap="false"
           memoryStoreEvictionPolicy="LRU" >

        <persistence strategy="none" />
    </cache>
    
    <cache name="historical.cache" 
            maxElementsInMemory="500" 
            eternal="false"
            overflowToDisk="false"
            overflowToOffHeap="false"
            memoryStoreEvictionPolicy="LRU" >

        <persistence strategy="none" />
    </cache>

   
   


</ehcache>
